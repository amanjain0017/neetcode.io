//using unordered maps
class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        unordered_map<int,bool> map;
        for(int i = 0; i<nums.size();i++){
            int curr = nums[i];
            if(map[curr]){
                return true;
            }
            else{
                map[curr] = true;
            }
        }
        return false;
    }
};